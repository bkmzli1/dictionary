<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="409.0" prefWidth="639.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="ru.bkmz.lib.controllers.Controller">

    <children>
        <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
              maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <MenuBar VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
                    </VBox.margin>
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#information" text="Information"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <Button fx:id="add" alignment="CENTER" contentDisplay="CENTER"
                                maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onAdd" text="Add" textAlignment="CENTER"
                                HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="delete" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onDelete" text="Delete" textAlignment="CENTER"
                                HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="edition" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onEdition" text="Edition" textAlignment="CENTER"
                                HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
                    </VBox.margin>
                </HBox>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <Button fx:id="update" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onUpdate" text="update" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="search" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onSearch" text="Search" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="get" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#onGet" text="get" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
                    </padding>
                </HBox>
                <TableView fx:id="table" editable="true" maxHeight="1.7976931348623157E308"
                           maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </TableView>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>

                        <ProgressBar fx:id="progresBar" accessibleHelp="11" accessibleRoleDescription="11"
                                     accessibleText="11" cache="true" cacheHint="SPEED"
                                     maxWidth="1.7976931348623157E308" mouseTransparent="true" progress="0.0"
                                     HBox.hgrow="ALWAYS">
                            <padding>
                                <Insets left="5.0"/>
                            </padding>
                        </ProgressBar>
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
            </padding>
        </VBox>
        <Text fx:id="text" AnchorPane.bottomAnchor="4.0" AnchorPane.leftAnchor="10.0"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</AnchorPane>
