<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="412.0" prefWidth="771.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            fx:controller="ru.bkmz.lib.controllers.ControllerSearch">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <children>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <children>
                        <TextArea fx:id="comintTex" cacheShape="false" centerShape="false" focusTraversable="false"
                                  maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                  prefHeight="248.0" prefWidth="170.0" promptText="definition" scaleShape="false">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </TextArea>
                        <TableView fx:id="table" editable="true" maxHeight="1.7976931348623157E308"
                                   maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" HBox.hgrow="ALWAYS">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </HBox>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"/>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <children>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <children>
                                <VBox VBox.vgrow="ALWAYS">
                                    <children>
                                        <HBox VBox.vgrow="ALWAYS">
                                            <children>
                                                <ToggleButton fx:id="nameButton" maxWidth="1.7976931348623157E308"
                                                              mnemonicParsing="false" onAction="#nameButton"
                                                              selected="true" text="on name" HBox.hgrow="ALWAYS"/>
                                                <ToggleButton fx:id="dictionaryButoon" maxWidth="1.7976931348623157E308"
                                                              mnemonicParsing="false" onAction="#dictionaryButoon"
                                                              text="off dictionary" HBox.hgrow="ALWAYS"/>
                                            </children>
                                        </HBox>
                                        <HBox VBox.vgrow="ALWAYS">
                                            <children>
                                                <ToggleButton fx:id="idButoon" maxHeight="1.7976931348623157E308"
                                                              maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                                              onAction="#idButoon" text="off id" HBox.hgrow="ALWAYS"/>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </VBox>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <children>
                                <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                        mnemonicParsing="false" onAction="#onSearch" text="search" VBox.vgrow="ALWAYS"/>
                                <Button fx:id="menuButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                                        onAction="#menuButton" text="menu"/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
